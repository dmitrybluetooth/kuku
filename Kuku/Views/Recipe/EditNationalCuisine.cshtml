@model Kuku.Models.NationalCuisine
@{
    ViewBag.Title = "Edit nationality Cuisine";
}
<form asp-action="EditNationalCuisine" asp-controller="Recipe" asp-route-nationalcuisineid="@Model.NationalCuisineId">
    <div class="form-group">
        <label asp-for="NationalCuisineName" class="control-label">Nationality cuisine name</label>
        <input type="text" asp-for="NationalCuisineName" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>